				} 

	this.set_icon_container = function(){
		var i = 0;
		$('#raphicon_repository').each(function(){
			i++
		})

		if(i == 0){
			$('body').append("<div style='display:none;' id='raphicon_repository'></div>")
		}
	}

	this.set_defaults = function(){
		var me = this
		if(!this.opts){
			throw new Error("Raphicon expected 1 argument, provided 0")
			return false
		}



		if(this.opts.substring){
			this.defaults.icon = this.opts
		} else {
			if(!this.opts.icon){
				throw new Error("You must specify a path to Raphicon")
				return false
			}

			for(option in this.opts){

				switch(option){
					case 'class':
						me.defaults['element_class'] = me.opts[option]
					break;

					default:
						me.defaults[option] = me.opts[option]
					break
				}
			}
		}
		
	}

	this.set_html = function(){
		var i = 0;
		var me = this;
		$('.repo_raphicon').each(function(){
			var tmp_id = this.id.replace('repo_raphicon', '') * 1
			if(tmp_id > i){
				i = tmp_id
			}
		})

		var repo_id = 'repo_raphicon' + (i + 1)
		var id ='raphicon' + (i + 1)

		// if the user has specified their own unique id
		if(this.defaults.id){
			id = this.defaults.id
		}
		
		
		if(this.defaults.return_type == 'string'){
			$('#raphicon_repository').append("<div class='repo_raphicon' id='"+repo_id+"'></div>")
			this.element = document.getElementById(repo_id);	
		} else {
			this.element = document.getElementById('tmp_raphicon_object')
			this.element.id = id
		}

		
		this.icon = new Raphael(this.element, this.defaults.width, this.defaults.height)
		
		this.path = this.icon
						  .path(this.icons[this.defaults.icon])
						  .attr({
							   fill: this.defaults.fill, 
							   stroke: this.defaults.stroke, 
							   'stroke-width': this.defaults.stroke_width,
							   width: this.defaults.width,
							   height: this.defaults.height
						  })

		//handle basic elemt css
		$(this.element).css({
			cursor:'pointer',
			float:this.defaults.float,
			margin:this.defaults.margin
		})

		var width = this.path.getBBox().width
		var height = this.path.getBBox().height
		
		var height_ratio = this.defaults.height / height
		var width_ratio = this.defaults.width / width

		var margin_left = width_ratio * this.path.getBBox().x
		var margin_top = height_ratio * this.path.getBBox().y

		if(height >= width){
			var ratio = height_ratio
		}else{
			var ratio = width_ratio
		}
		this.path.transform('s' + ratio)
		var x = (this.path.getBBox().x * -1) 
		var y = (this.path.getBBox().y * -1) 

	
		this.path.transform('t' + x + ', ' + y + ', s' + ratio)

		if(this.defaults.element_class){
			if(this.defaults.element_class.substr){
				$(this.element).addClass(this.defaults.element_class)
			} else {
				for(item in this.defaults.element_class){
					$(this.element).addClass(this.defaults.element_class[item])
				}
			}
		}

		if(this.defaults.return_type == 'string'){
			if(this.defaults.string_format == 'outerHTML'){
				return this.element.outerHTML
			} else {
				return this.element.innerHTML
			}
		}
	}

	

	this.initialize = function(){
		this.set_icon_container()
		this.set_defaults()
		if(this.defaults.return_type == 'string'){
			return this.set_html()
		} else {
			this.set_html()
		}
	}

	if(this.defaults.return_type == 'string'){
		return this.initialize()
	} else {
		this.initialize()
	}

	
}
;
